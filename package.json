{
  "private": true,
  "name": "boostlye_survey",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "lint": "eslint src",
    "deploy": "preact build --no-prerender && aws s3 sync build/ s3://boostly-survey/ --acl public-read"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "^4.5.0",
    "eslint-config-synacor": "^1.1.0",
    "eslint-plugin-react": "^7.5.0",
    "if-env": "^1.0.0",
    "preact-cli": "^2.0.0"
  },
  "dependencies": {
    "boostly-ui": "^1.4.0",
    "glamor": "^2.20.40",
    "glamorous": "^4.11.0",
    "preact": "^8.2.1",
    "query-string": "^5.0.1",
    "react-media": "^1.6.1",
    "react-modal": "^3.1.2"
  }
}
